<div class="article-page-container">
  <article class="article-page">
    <h2><%- categoryArticle.title.rendered %></h2>
    <img
      class="featured-image"
      src="<%= categoryArticle.yoast_head_json.og_image[0].url %>"
      alt="<%= categoryArticle.yoast_head_json.schema['@graph'][2].caption %>"
    />
    <p class="category-name"><%= categoryArticle.categories%></p>
    <p class="caption">
      <%= categoryArticle.yoast_head_json.schema['@graph'][2].caption %>
    </p>
    <span class="excerpt"><%- categoryArticle.excerpt.rendered %></span>

    <p class="reading-time">
      <%= categoryArticle.yoast_head_json.twitter_misc["Geschatte leestijd"] %>
    </p>
    <p class="date"><%= categoryArticle.date %></p>
    <div class="content"><%- categoryArticle.content.rendered %></div>
  </article>
</div>
